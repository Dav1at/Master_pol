<Window x:Class="Master_pol.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Master_pol"
        mc:Ignorable="d"
        Title="Мир полов" Height="450" Width="800" Icon="/Resource/ico_logo.ico">
    <Grid>
        
        <Grid.RowDefinitions>
            <RowDefinition Height="75"/>
            <RowDefinition Height="69*"/>
            <RowDefinition Height="30"/>
        </Grid.RowDefinitions>
        
        <Grid Grid.Row="0" Background="#F4E8D3"></Grid>
        <Grid Grid.Row="2" Background="#F4E8D3"></Grid>

        <Image Source="Resource/logo.png" Width="100" HorizontalAlignment="Left" Grid.ColumnSpan="2"></Image>
        <TextBlock Text="Список парнтеров" HorizontalAlignment="Center" Grid.ColumnSpan="2" FontFamily="Segoe UI" FontSize="24"></TextBlock>
        <Frame Name="FrameMain" NavigationUIVisibility="Hidden" Grid.Row="1" ></Frame>
    </Grid>
</Window>
